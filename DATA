                        - btnAgregar_4:
                            Control: Button@0.0.45
                            Properties:
                              Align: =Align.Center
                              DisplayMode: |-
                                =If(
                                    CountRows(galCierre.AllItems) = 0, 
                                    DisplayMode.Disabled, 
                                    DisplayMode.Edit
                                )
                              Icon: ="Cart"
                              IconStyle: ='ButtonCanvas.IconStyle'.Outline
                              LayoutMaxHeight: =0
                              LayoutMaxWidth: =0
                              LayoutMinHeight: =16
                              LayoutMinWidth: =16
                              OnSelect: "=// Recorremos la galería\r\nForAll(\r\n    galCierre.AllItems, // Asegúrate que este sea el nombre de tu galería en esta pantalla\r\n    \r\n    // A. Actualizar SharePoint\r\n    Patch(\r\n        DBASIG,\r\n        LookUp(DBASIG, ID = Value(lblIDFinal.Text)), \r\n        {\r\n            ESTADO: {Value: \"FINALIZADO\"}, // Estado Final\r\n            \r\n            // Datos de Cierre\r\n            'FIN REAL': dateFinReal.SelectedDate, // Fecha Fin\r\n            COMENTARIOS: txtComentariosFinal.Text, // Comentarios\r\n            RESPONSABLE: drpResponsableFinal.Selected.Value, // Responsable Final\r\n            \r\n            // AQUÍ ESTABA EL ERROR: Leemos la imagen cargada\r\n            FOTOEVIDENCIA: UploadedImage2.Image \r\n        }\r\n    );\r\n\r\n    // B. Enviar Correo de CIERRE\r\n    Office365Outlook.SendEmailV2(\r\n        \"2015-081484@intecap.edu.gt\", \r\n        \"✅ TAREA FINALIZADA: \" & ThisRecord.AREA,\r\n        \r\n        // Cuerpo del correo profesional\r\n        \"<h3>Reporte de Cierre de Actividad</h3>\" &\r\n        \"<p>El técnico <b>\" & drpResponsableFinal.Selected.Value & \"</b> ha completado la labor.</p>\" &\r\n        \"<ul>\" &\r\n            \"<li><b>Área:</b> \" & ThisRecord.AREA & \"</li>\" &\r\n            \"<li><b>Trabajo:</b> \" & ThisRecord.DESCRIPCION & \"</li>\" &\r\n            \"<li><b>Fecha Inicio:</b> \" & Text(ThisRecord.INICIOREAL, \"dd/mm/yyyy\") & \"</li>\" &\r\n            \"<li><b>Fecha Fin:</b> \" & Text(dateFinReal.SelectedDate, \"dd/mm/yyyy\") & \"</li>\" &\r\n            \"<li><b>Comentarios:</b> \" & txtComentariosFinal.Text & \"</li>\" &\r\n        \"</ul>\" &\r\n        \"<p style='color:green;'><b>ESTADO ACTUALIZADO A: FINALIZADO</b></p>\" &\r\n        \"<p><i>* La evidencia fotográfica ha sido cargada al sistema.</i></p>\"\r\n    )\r\n);\r\n\r\n// C. Limpieza\r\nClear(colParaFinalizar);\r\nNotify(\"¡Excelente! Tareas finalizadas.\", NotificationType.Success);\r\nNavigate(scrFINALIZAR, ScreenTransition.Cover)"
                              Text: ="CONFIRMAR (" & CountRows(galCierre.AllItems) & ") TAREAS"
